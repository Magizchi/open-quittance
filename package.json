{
  "name": "open-quittance",
  "version": "0.0.1",
  "private": true,
  "scripts": {
    "dev": "vite dev",
    "build": "vite build",
    "preview": "vite preview",
    "check": "svelte-kit sync && svelte-check --tsconfig ./tsconfig.json",
    "check:watch": "svelte-kit sync && svelte-check --tsconfig ./tsconfig.json --watch",
    "test": "vitest",
    "eslint": "eslint .",
    "lint-staged": "lint-staged",
    "prettier": "prettier --plugin-search-dir . --check .",
    "prettier:fix": "prettier --plugin-search-dir . --write .",
    "migrate": "npx drizzle-kit generate --config=drizzle.config.ts && npx drizzle-kit push --config=drizzle.config.ts",
    "prepare": "husky",
    "commit": "cz"
  },
  "devDependencies": {
    "@commitlint/cli": "^19.5.0",
    "@commitlint/config-conventional": "^19.5.0",
    "@commitlint/types": "^19.5.0",
    "@eslint/js": "^9.13.0",
    "@sveltejs/adapter-node": "^5.2.9",
    "@sveltejs/vite-plugin-svelte": "^4.0.0",
    "@sveltejs/kit": "^2.8.0",
    "@tailwindcss/forms": "^0.5.9",
    "@types/bcrypt": "^5.0.1",
    "@types/blob-stream": "^0.1.33",
    "@types/cookie": "^0.5.1",
    "@types/eslint__js": "^8.42.3",
    "@types/file-saver": "^2.0.5",
    "@types/jsonwebtoken": "^9.0.4",
    "@types/pdfmake": "^0.2.8",
    "autoprefixer": "^10.4.16",
    "conventional-changelog-atom": "^5.0.0",
    "cz-conventional-changelog": "^3.3.0",
    "drizzle-kit": "^0.26.2",
    "eslint": "^8.57.1",
    "eslint-config-standard": "^17.1.0",
    "eslint-plugin-format": "^0.1.2",
    "eslint-plugin-import": "^2.31.0",
    "eslint-plugin-n": "^16.6.2",
    "eslint-plugin-promise": "^6.6.0",
    "eslint-plugin-svelte": "^2.46.0",
    "globals": "^15.11.0",
    "husky": "^9.1.6",
    "lint-staged": "^15.2.10",
    "postcss": "^8.4.31",
    "prettier": "^3.3.3",
    "prettier-plugin-svelte": "^3.2.7",
    "svelte": "^5.1.13",
    "svelte-check": "^3.4.3",
    "svelte-eslint-parser": "^0.43.0",
    "tailwindcss": "^3.3.3",
    "tslib": "^2.4.1",
    "tsx": "^4.19.1",
    "typescript": "^5.6.3",
    "typescript-eslint": "^8.11.0",
    "vite": "^5.4.10",
    "vitest": "^0.34.0"
  },
  "type": "module",
  "dependencies": {
    "bcrypt": "^5.1.1",
    "blob-stream": "^0.1.3",
    "classnames": "^2.5.1",
    "convertir-nombre-lettre": "^1.0.2",
    "dayjs": "^1.11.13",
    "dotenv": "^16.4.5",
    "drizzle-orm": "^0.35.3",
    "file-saver": "^2.0.5",
    "jsonwebtoken": "^9.0.2",
    "mysql": "^2.18.1",
    "mysql2": "^3.11.3",
    "pdf-lib": "^1.17.1",
    "pdfmake": "^0.2.8"
  },
  "lint-staged": {
    "src/**/*": "prettier --write",
    "src/**/*.{svelte,ts}": "eslint"
  },
  "config": {
    "commitizen": {
      "path": "./node_modules/cz-conventional-changelog"
    }
  }
}
